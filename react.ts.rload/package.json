{
  "name": "ts",
  "version": "1.5.0",
  "description": "A simple template for creating react app using TypeScript with hot reload.",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --exclude 'src/tests/*.{ts,tsx}'",
    "start": "webpack-dev-server",
    "test": "jest",
    "build": "tsc && webpack --progress",
    "clean": "rd /s/q built",
    "prepare": "tsc && webpack --progress --config webpack.prod.config.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "antd": "^3.12.1",
    "axios": "^0.18.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "4.3.1",
    "react-router-config": "4.4.0-beta.6",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.12",
    "@types/react-router": "^4.4.3",
    "@types/react-router-config": "^1.1.0",
    "@types/react-router-dom": "4.3.1",
    "axios-mock-adapter": "^1.16.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "react-test-renderer": "^16.7.0",
    "redux-devtools-extension": "^2.13.7",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-import-plugin": "^1.5.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
